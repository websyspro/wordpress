{
	"name": "websyspro/wordpress",
	"type": "project",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Wordpress\\Themes\\": "src/"
		}
	},
	"scripts": {
		"install-wp": [
    	"vendor/bin/install"
		],
		"post-create-project-cmd": [
			"@composer dump-autoload",
			"@install-wp"
		]
  },
	"authors": [
		{
			"name": "Emerson Thiago",
			"email": "websyspro.services@gmail.com"
		}
	],
	"require": {
		"websyspro/commons": "dev-main",
		"websyspro/wpengine": "dev-main",
		"websyspro/devtools": "dev-main",
		"websyspro/elements": "dev-main"
	},
	"extra": {
		"wordpress": {
			"version": 6.9
		}
	}
}
